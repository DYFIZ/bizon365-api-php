{
    "name": "slavkluev/bizon365-api-php",
    "type": "library",
    "description": "",
    "keywords": [
        "slavkluev",
        "bizon365-api-php",
        "bizon365-api",
        "bizon365",
        "api"
    ],
    "homepage": "https://github.com/slavkluev/bizon365-api-php",
    "license": "MIT",
    "authors": [
        {
            "name": "Viacheslav Kliuev",
            "email": "slavkluev@yandex.ru",
            "homepage": "https://github.com/slavkluev",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "~7.1",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^6.5"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=7.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "slavkluev\\Bizon365\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "slavkluev\\Bizon365\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
